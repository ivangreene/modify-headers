{
  "name": "modify-headers",
  "private": true,
  "version": "0.1.0",
  "description": "Chrome extension to modify incoming/outgoing HTTP headers",
  "main": "index.js",
  "scripts": {
    "react-dev": "node scripts/dev",
    "dev": "npm-run-all -p css-watch react-dev",
    "css-build": "node-sass-chokidar --include-path ./node_modules app/ -o app/",
    "css-watch": "npm run css-build && node-sass-chokidar --include-path ./node_modules app/ -o app/ --watch --recursive",
    "build": "node scripts/build",
    "compress": "node scripts/compress",
    "compress-keygen": "crx keygen",
    "clean": "rimraf build/ dev/ *.zip *.crx",
    "lint": "eslint app chrome test scripts webpack/*.js",
    "test-e2e": "cross-env NODE_ENV=test mocha -r ./test/setup-app test/e2e",
    "test": "cross-env NODE_ENV=test mocha -r ./test/setup-app test/app",
    "test:watch": "cross-env NODE_ENV=test mocha --watch -r ./test/setup-app test/app"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivangreene/modify-headers.git"
  },
  "author": "Ivan Greene",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ivangreene/modify-headers/issues"
  },
  "homepage": "https://github.com/ivangreene/modify-headers#readme",
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.3.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "bulma": "^0.7.1",
    "chai": "^4.1.2",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "css-modules-require-hook": "^4.2.3",
    "jsdom": "^11.9.0",
    "mocha": "^5.1.1",
    "node-sass-chokidar": "^1.2.2",
    "npm-run-all": "^4.1.2",
    "postcss-loader": "^2.1.4",
    "pug-cli": "^1.0.0-alpha6",
    "react-addons-test-utils": "^15.6.2",
    "react-dock": "^0.2.4",
    "rimraf": "^2.6.2",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "shelljs": "^0.8.1",
    "sinon": "^4.5.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-hot-middleware": "^2.22.1",
    "webpack-httpolyglot-server": "^0.3.0"
  }
}
